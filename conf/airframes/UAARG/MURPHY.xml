<!DOCTYPE airframe SYSTEM "../airframe.dtd">

<!--
	Airframe file for MURPHY.  Created by Paul FEB, 2014

	The file compiles and can be flashed onto murphy. But all values are copied over from Telemaster so do not attempt to fly.
	

	
-->


<airframe name="Murphy">

<!--  *******************************************FIRMWARE****************************************************************** -->
<firmware name="fixedwing">
	<target name="ap" board="lisa_m_2.0"/>
    	<target name="sim" board="pc"/>
	<subsystem name="imu" type="aspirin_v2.2"/>
    	<subsystem name="ins" type="alt_float"/>
   	<subsystem name="radio_control" type="spektrum">
        	<define 	name="RADIO_MODE" value="RADIO_AUX2"/>
		<configure 	name="USE_SECONDARY_SPEKTRUM_RECEIVER" value="1"/> <!-- use 2 spektrum receivers -->
    	</subsystem>
    	<subsystem name="gps" 		type="ublox" />
    	<subsystem name="telemetry" 	type="transparent">
      		<configure name="MODEM_BAUD" value="B57600"/>
    	</subsystem>
	<subsystem name="control"/>
    	<!-- <subsystem name="navigation"/>  -->				<!-- used when there are no advanced flight routines -->
    	<subsystem name="navigation" 	type="extra"/> 		<!-- needed for advanced flight routines such as poly-survey, etc... -->
	<subsystem name="ahrs" 		type="float_cmpl_quat">
      		<define name="F_PROPAGATE_QUAT"/>
    	</subsystem>
</firmware>

<!-- *******************************************SERVOS****************************************************************** -->
<!-- 	defines the physical servos connected to the LISA M. ideally: min=1000 neutral=1500 max=2000	-->
<servos>	
    	<servo name="THROTTLE" 		no="0" 	min="1350" 	neutral="1350" 	max="2000"/>
    	<servo name="AILERON_LEFT" 	no="4" 	min="1200" 	neutral="1554" 	max="1874"/>
    	<servo name="AILERON_RIGHT" 	no="3" 	min="1189" 	neutral="1508" 	max="1800"/> <!--both left and right need to adjusted in the same direction -->	
    	<servo name="ELEVATOR" 		no="2" 	min="1000" 	neutral="1509" 	max="2000"/>	<!--min is up, max is down. 1580 original, moved to 1624-->
    	<servo name="RUDDER" 		no="5" 	min="1864" 	neutral="1382" 	max="900"/> <!--min is right, max is left -->
	<servo name="DROP" 		no="1" 	min="1000" 	neutral="1500" 	max="2000"/>
</servos>

<!-- *******************************************COMMANDS****************************************************************** -->
<!-- 	 Defines abstract commands needed to control aircraft	-->
<commands>
    	<axis name="THROTTLE" 	failsafe_value="0"/> <!-- range is 0 to 9600 -->
    	<axis name="ROLL" 	failsafe_value="0"/> <!-- range is -9600 to 9600 -->
    	<axis name="PITCH" 	failsafe_value="0"/> <!-- range is -9600 to 9600 -->
    	<axis name="BRAKE" 	failsafe_value="0"/> <!-- range is -9600 to 9600 -->
    	<axis name="YAW" 	failsafe_value="0"/> <!-- range is -9600 to 9600 -->
	<axis name="GEAR" 	failsafe_value="0"/> <!-- range is -9600 to 9600 -->

</commands>

<!-- *******************************************COMMAND LAWS****************************************************************** -->
<!-- 	links the abstract commands to physical servos 	-->
<command_laws>	
    <!--<set servo="AILERON_LEFT" 	value="@ROLL+0.5*(@BRAKE-50)"/>
    	<set servo="AILERON_RIGHT" 	value="@ROLL-0.5*@BRAKE"/>			-->
    	<set servo="AILERON_LEFT" 	value="@ROLL"/>
    	<set servo="AILERON_RIGHT" 	value="@ROLL"/>
    	<set servo="THROTTLE" 		value="@THROTTLE"/>
    	<set servo="ELEVATOR" 		value="@PITCH"/>
    	<set servo="RUDDER" 		value="@YAW"/>
	<set servo="DROP" 		value="@GEAR"/>
</command_laws>

<!-- *******************************************RC COMMANDS****************************************************************** -->
<!-- 	defines what abstract commands the RC signals control (while in manual mode)	-->
<rc_commands>	
    	<set command="THROTTLE" 	value="@THROTTLE"/>
    	<set command="ROLL" 		value="@ROLL"/>
    	<set command="PITCH" 		value="@PITCH"/>
    	<set command="YAW" 		value="@YAW"/>
	<set command="GEAR" 		value="@GEAR"/>
	
	<!--<set command="GEAR" 		value="@KILL"/>	-->
</rc_commands>

<!-- *******************************************AUTO RC COMMANDS****************************************************************** -->
<!-- 	defines what abstract commands the RC signals control while in auto mode	-->
<auto_rc_commands> 
    	<set command="YAW" 		value="@YAW"/>
	<set command="GEAR" 		value="@GEAR"/>
</auto_rc_commands>

<!-- *******************************************INS****************************************************************** -->
<!-- 		-->

<section name="INS" prefix="INS_">
    	<define name="ROLL_NEUTRAL_DEFAULT" 	value="0.0" unit="deg"/>
    	<define name="PITCH_NEUTRAL_DEFAULT" 	value="2.0" unit="deg"/>
</section>

<!-- *******************************************IMU****************************************************************** -->
<!-- 	Define orientation of IMU inside airframe and tune magnemeter	-->
<section name="IMU" prefix="IMU_">
    	<define name="BODY_TO_IMU_PHI"   	value="95" 	unit="deg"/> 		<!-- rotation about the x axis	-->
    	<define name="BODY_TO_IMU_THETA" 	value="3.0" 	unit="deg"/> 		<!-- rotation about the y axis	-->
    	<define name="BODY_TO_IMU_PSI"   	value="180.0" 	unit="deg"/> 		<!-- rotation about the z axis	-->
    	<define name="MAG_X_SENS" 		value="3.55154786286" integer="16"/>
    	<define name="MAG_Y_SENS" 		value="3.48065717613" integer="16"/>
    	<define name="MAG_Z_SENS" 		value="4.02365310041" integer="16"/>
 
    	<define name="MAG_X_NEUTRAL" value="157"/>
    	<define name="MAG_Y_NEUTRAL" value="-93"/>
    	<define name="MAG_Z_NEUTRAL" value="-574"/>
  </section>

 <!-- *******************************************AHRS****************************************************************** -->
<!-- 		-->
<section name="AHRS" prefix="AHRS_">
    	<define name="H_X" value=" 0.51562740288882"/>
    	<define name="H_Y" value="-0.05707735220832"/>
    	<define name="H_Z" value=" 0.85490967783446"/>
</section>

<!-- *******************************************AUTO 1****************************************************************** -->
<!-- 		-->
<section name="AUTO1" prefix="AUTO1_">
    	<define name="MAX_ROLL" 	value="27" 	unit="deg"/>
    	<define name="MAX_PITCH" 	value="25" 	unit="deg"/>
</section>

<!-- *******************************************BATTERY****************************************************************** -->
<!-- 		-->
<section name="BAT">
  	<define name="MilliAmpereOfAdc(adc)" 	value="((adc) - 505) * 124.0"/>
    	<define name="LOW_BAT_LEVEL" 		value="6.1" 	unit="V"/>
	<define name="MAX_BAT_LEVEL" 		value="6.9" 	unit="V"/>
    	<define name="CRITIC_BAT_LEVEL" 	value="6.0" 	unit="V"/>
    	<define name="CATASTROPHIC_BAT_LEVEL" 	value="5.9" 	unit="V"/>
	<!--<define name="VoltageOfAdc(adc)" 	value="adc * 0.011328388 "/>-->
</section>

<!-- *******************************************VERTCAL CONTROL****************************************************************** -->
<!-- 		-->
<section name="VERTICAL CONTROL" prefix="V_CTL_">
    	<define name="POWER_CTL_BAT_NOMINAL" 			value="5.8" 	unit="volt"/>
    	<!-- outer loop proportional gain -->
    	<define name="ALTITUDE_PGAIN" 				value="0.03"/>
    	<!-- outer loop saturation -->
    	<define name="ALTITUDE_MAX_CLIMB" 			value="3.0"/>

    	<!-- auto throttle inner loop -->
    	<define name="AUTO_THROTTLE_NOMINAL_CRUISE_THROTTLE" 	value="0.5"/>
    	<define name="AUTO_THROTTLE_MIN_CRUISE_THROTTLE" 	value="0.416666666667"/>
    	<define name="AUTO_THROTTLE_MAX_CRUISE_THROTTLE" 	value="0.592"/>
    	<define name="AUTO_THROTTLE_LOITER_TRIM" 		value="50"/>
    	<define name="AUTO_THROTTLE_DASH_TRIM" 			value="-200"/>
    	<define name="AUTO_THROTTLE_CLIMB_THROTTLE_INCREMENT" 	value="0.15" 	unit="%/(m/s)"/>
    	<define name="AUTO_THROTTLE_PGAIN" 			value="0.16"/>
    	<define name="AUTO_THROTTLE_IGAIN" 			value="0.05"/>
    	<define name="AUTO_THROTTLE_PITCH_OF_VZ_PGAIN" 		value="0.8"/>

    	<define name="THROTTLE_SLEW_LIMITER" 			value="2" 	unit="s"/>
</section>

<!-- *******************************************HORIZONTAL CONTROL****************************************************************** -->
<!-- 		-->
<section name="HORIZONTAL CONTROL" prefix="H_CTL_">
    	<define name="COURSE_PGAIN" 			value="0.6"/>
    	<define name="COURSE_DGAIN" 			value="0.3"/>
    	<define name="COURSE_PRE_BANK_CORRECTION" 	value="0.2"/>
    	<define name="ROLL_MAX_SETPOINT" 		value="60" 	unit="degrees"/>
    	<define name="ROLL_MIN_SETPOINT" 		value="-45" 	unit="degrees"/>
    	<define name="PITCH_MAX_SETPOINT" 		value="35" 	unit="degrees"/>
    	<define name="PITCH_MIN_SETPOINT" 		value="-35" 	unit="degrees"/>

    	<define name="PITCH_PGAIN" 			value="10000."/> 	<!-- adjusts pitch from current pitch to desired pitch-->
    	<define name="PITCH_DGAIN" 			value="50"/> 		<!-- takes the derivative of the pitch and predicts how to adjust accordingly-->

    	<define name="ELEVATOR_OF_ROLL" 		value="4000."/>

    	<define name="ROLL_ATTITUDE_GAIN" 		value="40140."/> <!-- was 2000 -->
    	<define name="ROLL_RATE_GAIN" 			value="1500."/>
</section>

<!-- *******************************************AGRESSIVE****************************************************************** -->
<!-- 		-->
<section name="AGGRESSIVE" prefix="AGR_">
    	<define name="BLEND_START" 		value="20"/>				<!-- Altitude Error to Initiate Aggressive Climb CANNOT BE ZERO!!-->
    	<define name="BLEND_END" 		value="10"/>				<!-- Altitude Error to Blend Aggressive to Regular Climb Modes  CANNOT BE ZERO!!-->
    	<define name="CLIMB_THROTTLE" 		value="0.63"/>				<!-- Gaz for Aggressive Climb -->
    	<define name="CLIMB_PITCH" 		value="25." 	unit="degrees" />	<!-- Pitch for Aggressive Climb -->
    	<define name="DESCENT_THROTTLE" 	value="0.005"/>				<!-- Gaz for Aggressive Decent -->
    	<define name="DESCENT_PITCH" 		value="-25." 	unit="degrees"/>	<!-- Pitch for Aggressive Decent -->
    	<define name="CLIMB_NAV_RATIO" 		value="0.8"/>				<!-- Percent Navigation for Altitude Error Equal to Start Altitude -->
    	<define name="DESCENT_NAV_RATIO"	value="1.0"/>
</section>

<!-- *******************************************FAILSAFE****************************************************************** -->
<!-- 		-->
<section name="FAILSAFE" prefix="FAILSAFE_">
    	<define name="DEFAULT_THROTTLE" 	value="13.5" 	unit="%"/>
    	<define name="DEFAULT_ROLL" 		value="12.0" 	unit="deg"/>
    	<define name="DEFAULT_PITCH" 		value="-6" 	unit="deg"/>
    	<define name="HOME_RADIUS" 		value="DEFAULT_CIRCLE_RADIUS" unit="m"/>
    	<define name="KILL_MODE_DISTANCE" 	value="(MAX_DIST_FROM_HOME*1.5)"/>
    	<define name="DELAY_WITHOUT_GPS" 	value="3" 	unit="s"/>
</section>

<!-- *******************************************MISC****************************************************************** -->
<!-- 	THE JUNK DRAWER. A LOT OF THESE ARE FROM TELEMASTER AND NEED TO BE INVESTIGATED	-->
<section name="MISC">
    	<define name="NOMINAL_AIRSPEED" 	value="12." 	unit="m/s"/>
    	<define name="CARROT" 			value="5." 	unit="s"/>
    	<define name="CONTROL_FREQUENCY" 	value="60" 	unit="Hz"/>
    	<define name="ALT_KALMAN_ENABLED" 	value="FALSE"/>
    	<define name="DEFAULT_CIRCLE_RADIUS" 	value="80.0"/>
    	<define name="GLIDE_AIRSPEED" 		value="10"/>
    	<define name="GLIDE_VSPEED" 		value="3."/>
    	<define name="GLIDE_PITCH" 		value="45" 	unit="deg"/>
</section>

<!-- *******************************************MODULES****************************************************************** -->
<!-- 	plug madulues in here	-->
<modules>
</modules>

</airframe>
