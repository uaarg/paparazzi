      <path approaching_time="3" wpts=" swp5, aswp5"/>
      <path approaching_time="4" wpts=" aswp5, swp6"/>
      <path approaching_time="3" wpts=" swp6, bswp7"/>
      <path approaching_time="3" wpts=" bswp7, swp7"/>
      <path approaching_time="4" wpts=" swp7, aswp7"/>
      <path approaching_time="3" wpts=" aswp7, bswp8"/>
      <path approaching_time="3" wpts=" bswp8, swp8"/>
      <path approaching_time="4" wpts=" swp8, aswp8"/>
      <path approaching_time="3" wpts=" aswp8, bswp9"/>
      <path approaching_time="3" wpts=" bswp9, swp9"/>
      <path approaching_time="4" wpts=" swp9, aswp9"/>
      <path approaching_time="3" wpts=" aswp9, bswp10"/>
      <path approaching_time="3" wpts=" bswp10, swp10"/>
      <path approaching_time="4" wpts=" swp10, aswp10"/>
      <path approaching_time="3" wpts=" aswp10, bswp11"/>
      <path approaching_time="3" wpts=" bswp11, swp11"/>
      <path approaching_time="4" wpts=" swp11, aswp11"/>
      <deroute block="LKN Flyover"/>
    </block>
    <block name="Extraz">
      <go wp="a"/>
      <circle radius="nav_radius" until="stage_time > 17" wp="b"/>
      <go approaching_time="3" wp="_4sa"/>
      <deroute block="LKN Flyover"/>
    </block>
    <block name="LKN Flyover">
      <go approaching_time="0" wp="bLKN"/>
      <path approaching_time="2" wpts="bLKN, LKN"/>
      <while cond="TRUE">
        <circle radius="nav_radius " until="stage_time > 43" wp="LKN"/>
        <circle radius="nav_radius + 30" until="stage_time > 43+31.4193*5/13" wp="LKN"/>
      </while>
      <deroute block="OAT Rising Circle"/>
    </block>
    <block name="Go aLKN">
      <go wp="aLKN"/>
      <deroute block="OAT Rising Circle"/>
    </block>
    <block name="OAT Rising Circle">
      <go wp="bOAT"/>
      <while cond="TRUE">
        <circle height="60+ 00" radius="nav_radius" until="stage_time > 43" wp="OAT"/>
        <circle height="60+ 50" radius="nav_radius" until="stage_time > 43" wp="OAT"/>
      </while>
      <deroute block="Standby"/>
    </block>
    <block name="Go aOAT">
      <go wp="aOAT"/>
      <deroute block="Standby"/>
    </block>
