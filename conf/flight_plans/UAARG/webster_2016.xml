  <blocks>
    <block name="Wait GPS">
      <while cond="!GpsFixValid()"/>
      <set value="1" var="kill_throttle"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
    </block>
    <block name="Holding point">
      <set value="1" var="kill_throttle"/>
      <attitude roll="0" throttle="0" vmode="throttle"/>
    </block>
    <block group="home" key="t" name="Takeoff" strip_button="Takeoff (wp CLIMB)" strip_icon="takeoff.png">
      <exception cond="GetPosAlt() > GetAltRef()+25" deroute="Standby"/>
      <set value="0" var="kill_throttle"/>
      <set value="0" var="autopilot_flight_time"/>
      <go from="HOME" pitch="15" throttle="1.0" vmode="throttle" wp="CLIMB"/>
    </block>
    <block group="home" key="Ctrl+a" name="Standby" strip_button="Standby" strip_icon="home.png">
      <circle height="100" radius="nav_radius" wp="STDBY"/>
    </block>
    <block name="WP_HOME">
      <circle radius="nav_radius" wp="HOME"/>
    </block>
    <block name="General_flight">
      <go approaching_time="3.5" wp="wp1"/>
      <path approaching_time="3" wpts=" wp1, bwp2"/>
      <path approaching_time="3" wpts=" bwp2, wp2"/>
      <path approaching_time="2.5" wpts=" wp2 , bwp3"/>
      <path approaching_time="2.5" wpts=" bwp3 , wp3"/>
      <path approaching_time="3" wpts=" wp3 , awp3"/>
      <path approaching_time="2.5" wpts=" awp3, bwp4"/>
      <path approaching_time="2" wpts=" bwp4, wp4"/>
      <path approaching_time="3" wpts=" wp4, awp4"/>
      <path approaching_time="2.5" wpts=" awp4, wp5"/>
      <path approaching_time="3" wpts=" wp5, wp6"/>
      <deroute block="Search_Sweep"/>
    </block>
    <block name="Search_Sweep">
      <path approaching_time="3" wpts=" wp6, swp1"/>
      <path approaching_time="3" wpts=" swp1, swp2"/>
      <path approaching_time="3" wpts=" swp2 , swp3"/>
      <path approaching_time="3" wpts=" swp3, bswp4"/>
      <path approaching_time="4" wpts=" bswp4, swp4"/>
      <path approaching_time="3" wpts=" swp4, aswp4"/>
      <path approaching_time="3" wpts=" aswp4, bswp5"/>
